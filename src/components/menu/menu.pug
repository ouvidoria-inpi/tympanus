mixin menu
  - let className = prefix + 'menu'
  div(class=className)&attributes(attributes)
    if block
      block

mixin menu-header
  .menu-header&attributes(attributes)
    .menu-title
      img(src="data:image/png;base64," alt="Imagem ilustrativa")
      span Identificação do site ou Sistema
    .menu-close
      +button.circle(aria-label="Fechar o menu" data-dismiss="menu")
        +icon.fa-times

mixin menu-footer
  .menu-footer&attributes(attributes)
    .menu-logos
      img(src="data:image/png;base64," alt="Imagem ilustrativa")
      img(src="data:image/png;base64," alt="Imagem ilustrativa")
    .menu-links
      a(href="javascript: void(0)")
        span.mr-1 Link externo 1
        +icon.fa-external-link-square-alt
      a(href="javascript: void(0)")
        span.mr-1 Link externo 2
        +icon.fa-external-link-square-alt
    .social-network
      .social-network-title Redes Sociais
      .d-flex
        a.br-button.circle(href="javascript: void(0)" aria-label="Compartilhar por Facebook")
          i.fab.fa-facebook-f(aria-hidden="true")
        a.br-button.circle(href="javascript: void(0)" aria-label="Compartilhar por Twitter")
          i.fab.fa-twitter(aria-hidden="true")
        a.br-button.circle(href="javascript: void(0)" aria-label="Compartilhar por Linkedin")
          i.fab.fa-linkedin-in(aria-hidden="true")
        a.br-button.circle(href="javascript: void(0)" aria-label="Compartilhar por Whatsapp")
          i.fab.fa-whatsapp(aria-hidden="true")
    .menu-info
      .text-center.text-down-01
        | Todo o conteúdo deste site está publicado sob a licença <strong>Creative Commons Atribuição-SemDerivações 3.0</strong>

mixin menu-offcanvas
  +menu&attributes(attributes)(id=attributes.id ? attributes.id : 'main-navigation')
    .menu-container
      .menu-panel
        +menu-header
        +menu-groupedby-dropdown
        +menu-footer
      .menu-scrim(data-dismiss="menu" tabindex="0")

mixin menu-push
  +menu.push&attributes(attributes)(id=attributes.id ? attributes.id : 'main-navigation')
    .menu-container
      .menu-panel
        +menu-header(data-visible="false")
        +menu-groupedby-dropdown
        +menu-footer(data-visible="false")
      .menu-scrim(data-dismiss="menu" tabindex="0")

mixin menu-contextual
  +menu.contextual
    .menu-trigger.d-sm-none
      +button.primary.block.py-4(data-toggle="contextual" aria-expanded=true)
        span.mr-1 Menu Contextual
        +icon.fa-chevron-up.ml-5
    .menu-container.d-sm-block
      .menu-panel.col-sm-3
        nav.menu-body
          .menu-folder
            .menu-item
              span.content AGRUPAMENTO 1
            ul
              li(role="none")
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 1
              li(role="none")
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 1
              li(role="none")
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 1
            .menu-item
              span.content AGRUPAMENTO 2
            ul
              li
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 2
              li
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 2
              li
                a.menu-item(href="javascript: void(0)")
                  span.content Item do menu 2
      .menu-scrim(data-dismiss="menu" tabindex="0")

mixin menu-groupedby-label
  nav.menu-body
    - let folder = 1
    while folder <= 3
      .menu-folder.active
        .menu-item
          span.icon: +icon.fa-bell
          span.content Camada 1
        ul
          li
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-heart
              span.content Camada 2
          li
            a.menu-item(href="javascript: void(0)" aria-expanded="false")
              span.icon: +icon.fa-address-book
              span.content Camada 2
            ul
              li
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-book
                  span.content= `Camada 3`
              li
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-tree
                  span.content= `Camada 3`
                ul
                  li
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
                  li
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
                  li
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
              li
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-moon
                  span.content Camada 3
          li
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-archive
              span.content Camada 2
      - folder++

mixin menu-groupedby-divider
  nav.menu-body
    - let folder = 1
    while folder <= 3
      .menu-folder.active
        ul
          li(role="treeitem")
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-heart
              span.content Camada 2
          li(role="treeitem")
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-address-book
              span.content Camada 2
            ul(role="group")
              li(role="treeitem")
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-book
                  span.content= `Camada 3`
              li(role="treeitem")
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-tree
                  span.content= `Camada 3`
                ul(role="group")
                  li(role="treeitem")
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
                  li(role="treeitem")
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
                  li(role="treeitem")
                    a.menu-item(href="javascript: void(0)")
                      span.content Camada 4
              li
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-moon
                  span.content Camada 3
          li
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-archive
              span.content Camada 2
      - folder++

mixin menu-groupedby-dropdown
  nav.menu-body(role="tree")
    - let folder = 1
    while folder <= 3
      .menu-folder
        a.menu-item(href="javascript: void(0)"  role="treeitem")
          span.icon: +icon.fa-bell
          span.content Camada 1
        ul
          li
            a.menu-item(href="javascript: void(0)" role="treeitem")
              span.icon: +icon.fa-heart
              span.content Camada 2
          li
            a.menu-item(href="javascript: void(0)" role="treeitem")
              span.icon: +icon.fa-address-book
              span.content Camada 2
            ul
              li
                a.menu-item(href="javascript: void(0)" role="treeitem")
                  span.icon: +icon.fa-book
                  span.content= `Camada 3`
              li
                a.menu-item(href="javascript: void(0)" role="treeitem")
                  span.icon: +icon.fa-tree
                  span.content= `Camada 3`
                ul
                  li
                    a.menu-item(href="javascript: void(0)" role="treeitem")
                      span.content Camada 4
                  li
                    a.menu-item(href="javascript: void(0)" role="treeitem")
                      span.content Camada 4
                  li
                    a.menu-item(href="javascript: void(0)" role="treeitem")
                      span.content Camada 4
              li
                a.menu-item(href="javascript: void(0)")
                  span.icon: +icon.fa-moon
                  span.content Camada 3
          li
            a.menu-item(href="javascript: void(0)")
              span.icon: +icon.fa-archive
              span.content Camada 2
      - folder++
    a.menu-item.divider(href="javascript: void(0)" role="treeitem")
      span.icon: +icon.fa-bell
      span.content Item de Camada 1
    a.menu-item.divider(href="javascript: void(0)" role="treeitem")
      span.icon: +icon.fa-bell
      span.content Item de Camada 1

mixin menu-groupedby
  .row
    .col-sm-4.col-lg-3.mb-5
      p Agrupamento por Expansão
      +menu.push.active
        .menu-container.position-static
          .menu-panel.h-auto.position-static.shadow-lg-right
            +menu-groupedby-dropdown
    .col-sm-4.col-lg-3.mb-5
      p Agrupamento por Rótulos
      +menu.push.active
        .menu-container.position-static.shadow-lg-right
          .menu-panel.h-auto.position-static
            +menu-groupedby-label
    .col-sm-4.col-lg-3.mb-5
      p Agrupamento por Dividers
      +menu.push.active
        .menu-container.position-static.shadow-lg-right
          .menu-panel.h-auto.position-static
            +menu-groupedby-divider

mixin menu-sizes
  .row.mb-5
    .col-sm-4.col-lg-3.mb-5
      p Densidade alta
      +menu.push.active.small
        .menu-container.position-static
          .menu-panel.h-auto.position-static.shadow-lg-right
            +menu-groupedby-dropdown
    .col-sm-4.col-lg-3.mb-5
      p Densidade baixa
      +menu.push.active.large
        .menu-container.position-static
          .menu-panel.h-auto.position-static.shadow-lg-right
            +menu-groupedby-dropdown
